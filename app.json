{
  "expo": {
    "owner": "kiaradiaz",
    "name": "WadaTrip Mobile",
    "slug": "wadatrip-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wadatrip.mobile",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "WadaTrip usa esta informaci√≥n para mejorar tu experiencia de viaje.",
        "UIBackgroundModes": ["remote-notification"],
        "NSLocationWhenInUseUsageDescription": "We use your location to tag community posts and show them on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wadatrip.mobile",
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "wadatrip",
    "plugins": [
      "expo-apple-authentication",
      "expo-font",
      "expo-localization",
      
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ff6600"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b79e31bd-c5f5-41fa-86a5-c456870090d3"
      },
      "auth": {
        "webClientId": "981114942208-lr5aqqov1ifftioc462ut45sadvvjusd.apps.googleusercontent.com",
        "androidClientId": "981114942208-8hsf2u8833vlpuia4l9jmeus9sdd650p.apps.googleusercontent.com",
        "iosClientId": ""
      },
      "stripe": {
        "publishableKey": "pk_live_51OzQpxAHNOOTJQKY92xoPWCzjNYLPF9VhiFrUEmhihlsghV7oCrxVxaEkrUneLA9wFJ6AoQ2OTrJRfwqpUU55Ma500ZHhFtWdW"
      }
    }
  }
}
