{
  "name": "wadatrip-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start",
    "dev": "npx expo start -c",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "android:go": "npx expo start --android",
    "seed": "node src/scripts/seedFirestore.js",
    "mock:backend": "node scripts/mock-backend.js",
    "android:reverse": "adb reverse tcp:3000 tcp:3000",
    "dev:lan": "cross-env EXPO_PUBLIC_API_BASE_URL=http://10.0.0.148:3000 EXPO_PUBLIC_API_MODE=live EXPO_PUBLIC_HTTP_TIMEOUT_MS=30000 expo start -c",
    "dev:reverse": "npm run android:reverse && cross-env EXPO_PUBLIC_API_BASE_URL=http://localhost:3000 EXPO_PUBLIC_API_MODE=live EXPO_PUBLIC_HTTP_TIMEOUT_MS=30000 expo start -c",
    "dev:agent": "npm run android:reverse && cross-env EXPO_PUBLIC_API_MODE=live EXPO_PUBLIC_API_BASE_URL=http://localhost:3000 EXPO_PUBLIC_ENABLE_WADA_AGENT=true EXPO_PUBLIC_HTTP_TIMEOUT_MS=30000 npx expo start --android -c",
    "dev:agent:lan": "cross-env EXPO_PUBLIC_API_MODE=live EXPO_PUBLIC_API_BASE_URL=http://10.0.0.148:3000 EXPO_PUBLIC_ENABLE_WADA_AGENT=true EXPO_PUBLIC_HTTP_TIMEOUT_MS=30000 npx expo start -c",
    "dev:agent:hub": "npm run android:reverse && cross-env EXPO_PUBLIC_API_MODE=live EXPO_PUBLIC_API_BASE_URL=http://localhost:3000 EXPO_PUBLIC_ENABLE_WADA_AGENT=true EXPO_PUBLIC_FF_PROVIDER_HUB=true EXPO_PUBLIC_DEFAULT_LANG=en EXPO_PUBLIC_HTTP_TIMEOUT_MS=30000 npx expo start --android -c"
  },
  "dependencies": {
    "@expo/metro-runtime": "3.2.3",
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "6.3.20",
    "@types/react": "18.2.79",
    "expo": "~51.0.39",
    "expo-apple-authentication": "~6.4.2",
    "expo-auth-session": "5.5.2",
    "expo-build-properties": "0.12.5",
    "expo-crypto": "~13.0.2",
    "expo-font": "12.0.10",
    "expo-linear-gradient": "~13.0.2",
    "expo-localization": "15.0.3",
    "expo-location": "~17.0.1",
    "expo-image-picker": "~15.1.0",
    "expo-notifications": "~0.28.19",
    "expo-system-ui": "3.0.7",
    "expo-web-browser": "13.0.3",
    "firebase": "10.14.1",
    "i18next": "25.3.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "15.6.1",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "0.19.10",
    "typescript": "5.3.3",
    "expo-dev-client": "~4.0.29"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "babel-preset-expo": "11.0.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5"
  },
  "private": true
}
